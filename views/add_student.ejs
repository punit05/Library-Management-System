<% include partials/header%>
<h1>Add a New Student</h1>
<form class ="container" action="/lms/add_student" method="POST">
    <input type="text" name="student_id" value="<%=Math.floor(Math.random() * 255);%>">
    <input type="text" name="name" placeholder="Student Name">
    <input type="text" name="stu_reg_no" placeholder="Student Registration No">
     <input type="text" name="fname" placeholder="Father Name">
     <input type="text" name="course" placeholder="Course">
      <input type="text" name="department" placeholder="Department">
       <input type="text" name="year" placeholder="Year">
       <input type="text" name="semester" placeholder="Semester">
       
       <button>Add New Student</button>
</form>
<% include partials/footer%>